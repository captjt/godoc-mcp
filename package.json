{
  "name": "godoc-mcp",
  "version": "0.1.0",
  "description": "MCP server for real-time Go package documentation from pkg.go.dev",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "godoc-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:reliable": "vitest run tests/integration/reliable.test.ts tests/integration/basic-functionality.test.ts tests/unit",
    "test:unit": "vitest run tests/unit",
    "test:core": "vitest run tests/core-functionality.test.ts tests/unit"
  },
  "keywords": [
    "mcp",
    "go",
    "golang",
    "documentation",
    "pkg.go.dev"
  ],
  "author": "captjt",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "cheerio": "^1.0.0-rc.12",
    "node-cache": "^5.1.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.55.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.6.1"
  }
}
